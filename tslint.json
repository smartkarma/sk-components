{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "member-access": false,
    "member-ordering": true,
    "no-bitwise": false,
    "no-empty-interface": false,
    "no-console": true,
    "one-variable-per-declaration": false,
    "ordered-imports": true,
    "quotemark": [true, "single"]
  }
}
